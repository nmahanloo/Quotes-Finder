  <%- include("partials/header.ejs") %>
  <body>
    
    <h1 class="title">Quotes Found</h1>
    <main>
      <form id="quotes" action="/updateLikes">
        <% for (i of quotes) { %>
            <span id="quote">
              <%= i.quote %>
            </span><br/>
            <em id="author-quote-list">
              <span>-
                <a href="#" class="author-sig" id='<%= i.authorId %>'>
                  <%= i.firstName %> <%= i.lastName %>
                </a>
              </span>
            </em>
            <span id="category-result">
              Category: <%= i.category %>
            </span><br/>
            <span id="likes">
              <%= i.likes %> likes
            </span>
            <br><br>
        <% } %>
      </form>
    </main>
    <div id="authorModal" class="modal" tabindex="-1">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h1 class="modal-title">Author Information</h1>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body">
            <div id="authorInfo"></div>
          </div>
          <div class="modal-footer">
            <button type="button" class="close" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
          </div>
        </div>
      </div>
    </div>
    <div id="back">
      <form id="back-button" action="/">
        <button class="close">Back</button>
      </form>
    </div>
    <br>
    <script src="/js/script.js"></script>  
    <%- include("partials/footer.ejs") %>
  
  </body>
</html>